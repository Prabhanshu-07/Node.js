<!DOCTYPE html>
<html>
  <head>
    <script src="https://cdn.tailwindcss.com"></script>
  </head>
  <body>
    <h1 class="bg-blue-500 text-white p-4 rounded-lg">User list</h1>
    <ul class="mt-4">
      <% people.forEach((person)=>{ %>
      <li class="bg-gray-200 p-2 my-2 rounded-md">
        <span><%= person.name %></span>
        <span><%= person.email %></span>
        <span><%= person.username %></span>
      </li>
      <%}); %>
    </ul>
  </body>
</html>
